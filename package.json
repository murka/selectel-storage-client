{
  "name": "selectel-storage-client-v2",
  "version": "0.1.2",
  "description": "Node.js client for Selectel Storage",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib/* && rimraf dist/* && rimraf coverage/*",
    "pre-commit": "lint-staged",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "keywords": [
    "selectel"
  ],
  "lint-staged": {
    "*.ts*": [
      "prettier --write",
      "git add"
    ]
  },
  "author": "Akiyakov Denis<newsiberian2015@yandex.ru>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/newsiberian/selectel-storage-client/issues"
  },
  "homepage": "https://github.com/newsiberian/selectel-storage-client#readme",
  "dependencies": {
    "got": "^10.7.0"
  },
  "devDependencies": {
    "@types/jest": "25.1.4",
    "@types/node": "13.9.5",
    "dotenv": "8.2.0",
    "faker": "4.1.0",
    "husky": "4.2.3",
    "jest": "25.2.3",
    "lint-staged": "10.0.9",
    "prettier": "2.0.2",
    "rimraf": "3.0.2",
    "ts-jest": "25.2.1",
    "ts-node": "8.8.1",
    "tslint": "6.1.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.8.3"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*\\.test\\.ts)$",
    "testPathIgnorePatterns": [
      "/dist/",
      "/node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ]
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "bracketSpacing": true,
    "trailingComma": "all",
    "parser": "typescript"
  },
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  }
}
